{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFE082",
          "style": "plain",
          "template": "@{{ .UserName }} \u279c",
          "type": "session"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#7FD5EA",
          "style": "powerline",
          "template": "(\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}) ",
          "type": "go"
        },
        {
          "foreground": "#9e7eff",
          "style": "powerline",
          "template": "(\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}) ",
          "type": "python"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#E8CC97>git(</>{{ .HEAD }}<#E8CC97>) </>",
          "type": "git"
        },
		{
          "foreground": "#D75F00",
          "style": "plain",
          "template": "Î» ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false,
  "version": 2
}
